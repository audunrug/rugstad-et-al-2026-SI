@article{hejdaImpactsInvasiveAlien2023,
  title = {Impacts of Invasive Alien Species on Riparian Plant Communities in {{South African}} Savanna},
  author = {Hejda, Martin and {\v C}uda, Jan and Py{\v s}kov{\'a}, Kl{\'a}ra and Foxcroft, Llewellyn C. and Nkuna, Khensani V. and Novoa, Ana and Py{\v s}ek, Petr},
