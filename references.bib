@article{arrheniusHelgoShadowDust2013,
  title = {Helg{\"o} in the Shadow of the Dust Veil 536-37},
  author = {Arrhenius, Birgit},
  year = {2013},
  journal = {Journal of Archaeology and Ancient History (JAAH)},
  number = {5},
  address = {Uppsala},
  issn = {20011199 (EISSN)},
  urldate = {2013-03-27},
  abstract = {In this paper, the implications of the dust veil in 536-37 AD at the site Helg{\"o} in Lake M{\"a}laren are investigated. One dramatic change at Helg{\"o} was the apparent abandonment around 530 of the Migration Period casting of bronze artefacts in Building group 3. A vast accumulation of casting moulds and crucibles was found here. The magnitude makes this context unique in northern Europe. Usually, the very fine-grained quarts sand used for the mould and crucibles was reused, and thus it would not accumulate. Another incident is that the open-air offering place, known to be in use already from the Later Roman Iron Age is abandoned. From this point, the cultic events are performed indoors in a large hall. Further, from this period onwards, the cemeteries on the site are commenced, indicating that the site now becomes permanently inhabited. Finally, although previously not known as a grave gift, bread is regularly found in graves.},
  langid = {english},
  keywords = {536/537,abandonment of casting place,bread as grave gift,cultic events indoors,Dust veil,Humaniora,Humanities,leaving an open air offering place,permanent inhabitation}
}

