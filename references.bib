@article{legendreTestingSignificanceCanonical2011,
  title = {Testing the Significance of Canonical Axes in Redundancy Analysis},
  author = {Legendre, Pierre and Oksanen, Jari and {ter Braak}, Cajo J. F.},
  year = {2011},
  journal = {Methods in Ecology and Evolution},
  volume = {2},
  number = {3},
  pages = {269--277},
  issn = {2041-210X},
  doi = {10.1111/j.2041-210X.2010.00078.x},
  urldate = {2025-05-27},
  abstract = {1. Tests of significance of the individual canonical axes in redundancy analysis allow researchers to determine which of the axes represent variation that can be distinguished from random. Variation along the significant axes can be mapped, used to draw biplots or interpreted through subsequent analyses, whilst the nonsignificant axes may be dropped from further consideration. 2. Three methods have been implemented in computer programs to test the significance of the canonical axes; they are compared in this paper. The simultaneous test of all individual canonical axes, which is appealing because of its simplicity, produced incorrect (highly inflated) levels of type I error for the axes following those corresponding to true relationships in the data, so it is invalid. The `marginal' testing method implemented in the `vegan' R package and the `forward' testing method implemented in the program CANOCO were found to have correct levels of type I error and comparable power. Permutation of the residuals achieved greater power than permutation of the raw data. 3. R functions found in a Supplement to this paper provide the first formal description of the `marginal' and `forward' testing methods.},
  copyright = {{\copyright} 2010 The Authors. Methods in Ecology and Evolution {\copyright} 2010 British Ecological Society},
  langid = {english},
  keywords = {canonical redundancy analysis (RDA),numerical simulations,power,tests of significance,type I error},
  file = {/Users/audunrug/Zotero/storage/EGI7DI6C/Legendre et al. - 2011 - Testing the significance of canonical axes in redu.pdf;/Users/audunrug/Zotero/storage/4YEXCGEZ/j.2041-210X.2010.00078.html}
}

@article{leiboldInternalStructureMetacommunities2022,
  title = {The Internal Structure of Metacommunities},
  author = {Leibold, Mathew A. and Rudolph, F. Javiera and Blanchet, F. Guillaume and De Meester, Luc and Gravel, Dominique and Hartig, Florian and {Peres-Neto}, Pedro and Shoemaker, Lauren and Chase, Jonathan M.},
  year = {2022},
  journal = {Oikos},
  volume = {2022},
  number = {1},
  issn = {1600-0706},
  doi = {10.1111/oik.08618},
  urldate = {2025-03-12},
  abstract = {Current analyses of metacommunity data largely focus on global attributes across the entire metacommunity, such as mean alpha, beta, and gamma diversity, as well as the partitioning of compositional variation into single estimates of contributions of space and environmental effects and, more recently, possible contributions of species interactions. However, this view neglects the fact that different species and sites in the landscape can vary widely in how they contribute to these metacommunity-wide attributes. We argue for a new conceptual framework with matched analytics with the goals of studying the complex and interactive relations between process and pattern in metacommunities that is focused on the variation among species and among sites which we call the `internal structure' of the metacommunity. To demonstrate how the internal structure could be studied, we create synthetic data using a process-based colonization--extinction metacommunity model. We then use joint species distribution models to estimate how the contributions of space, environment, and biotic interactions driving metacommunity assembly differ among species and sites. We find that this approach to the internal structure of metacommunities provides useful information about the distinct ways that different species and different sites contribute to metacommunity structure. Although it has limitations, our work points at a more general approach to understand how other possible complexities might affect internal structure and might thus be incorporated into a more cohesive metacommunity theory.},
  copyright = {{\copyright} 2021 Nordic Society Oikos. Published by John Wiley \& Sons Ltd},
