@article{reisLongtermEffectInitial2022,
  title = {The Long-Term Effect of Initial Restoration Intervention, Landscape Composition, and Time on the Progress of {{Pannonic}} Sand Grassland Restoration},
  author = {Reis, Bruna Paolinelli and Szit{\'a}r, Katalin and {K{\"o}vendi-Jak{\'o}}, Anna and T{\"o}r{\"o}k, Katalin and S{\'a}radi, N{\'o}ra and Cs{\'a}kv{\'a}ri, Edina and Halassy, Melinda},
  year = {2022},
