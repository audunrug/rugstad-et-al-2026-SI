@article{damgaardModelbasedOrdinationPinpoint2020,
  title = {Model-Based Ordination of Pin-Point Cover Data: {{Effect}} of Management on Dry Heathland},
  shorttitle = {Model-Based Ordination of Pin-Point Cover Data},
  author = {Damgaard, Christian and Hansen, Rikke Reisner and Hui, Francis K. C.},
  year = {2020},
  month = nov,
  journal = {Ecological Informatics},
  volume = {60},
  pages = {101155},
  issn = {1574-9541},
  doi = {10.1016/j.ecoinf.2020.101155},
  urldate = {2025-04-29},
  abstract = {Recently, there has been an increasing interest in model-based approaches for the statistical modelling of the joint distribution of multi-species abundances. The Dirichlet-multinomial distribution has been proposed as a suitable candidate distribution for the joint species distribution of pin-point plant cover data and is here applied in a model-based ordination framework. Unlike most model-based ordination methods, both fixed and random effects are in our proposed model structured as p-dimensional vectors and added to the latent variables before multiplying with the species-specific coefficients. This changes the interpretation of the parameters, so that the fixed and random effects now measure the relative displacement of the vegetation by the fixed and random factors in the p-dimensional latent variable space. This parameterization allows statistical inference of the effect of fixed and random factors in vector space, and makes it easier for practitioners to perform inferences on species composition in a multivariate setting. The method was applied on plant pin-point cover data from dry heathlands that had received different management treatments (burned, grazed, harvested, unmanaged), and it was found that treatment have a significant effect on heathland vegetation both when considering plant functional groups or when the taxonomic resolution was at the species level.},
  keywords = {Management of dry heathlands,Model-based ordination,Plant pin-point cover data,Statistical inferences on fixed and random effects},
  file = {/Users/audunrug/Zotero/storage/4GW5Y9I5/Damgaard et al. - 2020 - Model-based ordination of pin-point cover data Ef.pdf;/Users/audunrug/Zotero/storage/VQKRP5RI/S1574954120301059.html}
}

