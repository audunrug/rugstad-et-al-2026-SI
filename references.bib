@article{maccheriniDetectionEffectsRestoration2014,
  title = {Detection of the Effects of Restoration on Community Composition in a Calcareous Grassland: {{Does}} Scale Matter?},
  shorttitle = {Detection of the Effects of Restoration on Community Composition in a Calcareous Grassland},
  author = {Maccherini, Simona and Santi, Elisa and Marignani, Michela},
