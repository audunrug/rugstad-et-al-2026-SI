@article{rossiUpgradingModelSelection2020,
  title = {Upgrading {{Model Selection Criteria}} with {{Goodness}} of {{Fit Tests}} for {{Practical Applications}}},
  author = {Rossi, Riccardo and Murari, Andrea and Gaudio, Pasquale and Gelfusa, Michela},
  year = {2020},
