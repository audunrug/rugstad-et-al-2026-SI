@article{liSeasonalVariationStructure2022,
  title = {Seasonal Variation in Structure and Function of Gut Microbiota in {{Pomacea}} Canaliculata},
  author = {Li, Shuxian and Qian, Zijin and Yang, Jiani and Lin, Youfu and Li, Hong and Chen, Lian},
  year = {2022},
