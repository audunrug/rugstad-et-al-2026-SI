@article{lam-gordilloExtremeWeatherEvent2025,
  title = {Extreme Weather Event Causes Contrasting Macrobenthic Disturbance-Recovery Dynamics in Two {{New Zealand}} Estuaries},
  author = {{Lam-Gordillo}, Orlando and Petersen, Grady L. and Hailes, Sarah F. and Carter, Kelly and Salmond, Nichola H. and McCartain, Lisa and Ferries, Michal and Greenfield, Barry and Douglas, Emily J. and Hattingh, Reuben and Drylie, Tarn and Shanahan, Becky and Lohrer, Andrew M.},
  year = {2025},
  month = sep,
  journal = {Next Research},
  volume = {2},
  number = {3},
  pages = {100726},
  issn = {3050-4759},
  doi = {10.1016/j.nexres.2025.100726},
  urldate = {2025-09-12},
  abstract = {Extreme weather events, such as cyclones, are predicted to increase in frequency and intensity due to the rapidly changing climate. Cyclones can drastically alter estuarine ecosystems, affecting ecosystem function and service delivery. This study aimed to evaluate the effects of an extreme weather event, Cyclone Gabrielle, on estuarine macrobenthic communities and estuarine ecosystem functions in two estuaries in New Zealand. The data was analysed from prior to the passage of the cyclone (Before), soon after its passage (Event), and in the year after its passage (After), focusing on intertidal soft-sediment macroinvertebrate communities and metrics (e.g., abundance, richness, diversity, functional traits, functional richness, functional redundancy) and the overall relationship between macrobenthic fauna, their functional traits, and environmental conditions (i.e., estuarine ecosystem functioning). Results showed that the extreme weather event caused significant changes in macrobenthic metrics over time. Decreases in key macrobenthic metrics were coincident with the Event and After the cyclone in one of the estuaries. Results also showed changes in estuarine ecosystem functioning as macrobenthic metrics and functional traits changed, with reduced and weaker relationships found After the cyclone, suggesting potential disruption of key processes, functions, and services that estuarine ecosystems provide. Yet, these findings provide evidence of relatively rapid and nearly complete recovery to pre-cyclone ecological conditions within a year of the extreme weather event. More frequent extreme weather events might surpass the recovery periods of macrobenthic organisms and the environment, leading to greater impacts.},
  keywords = {Biological traits,Cyclone,Estuaries,Estuarine functioning,Functional diversity,Macrofauna,torm},
  file = {/Users/audunrug/Zotero/storage/R46FTVYN/Lam-Gordillo et al. - 2025 - Extreme weather event causes contrasting macrobenthic disturbance-recovery dynamics in two New Zeala.pdf;/Users/audunrug/Zotero/storage/7QT765GU/S3050475925005937.html}
}

