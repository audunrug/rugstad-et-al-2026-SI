@article{valkoHayTransferNaturebased2022,
  title = {Hay Transfer Is a Nature-Based and Sustainable Solution for Restoring Grassland Biodiversity},
  author = {Valk{\'o}, Orsolya and R{\'a}dai, Zolt{\'a}n and De{\'a}k, Bal{\'a}zs},
  year = {2022},
