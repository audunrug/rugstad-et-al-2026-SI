@article{jiangSolarForcingHolocene2015,
  title = {Solar Forcing of {{Holocene}} Summer Sea-Surface Temperatures in the Northern {{North Atlantic}}},
  author = {Jiang, Hui and Muscheler, Raimund and Bj{\"o}rck, Svante and Seidenkrantz, Marit-Solveig and Olsen, Jesper and Sha, Longbin and Sjolte, Jesper and Eir{\'i}ksson, J{\'o}n and Ran, Lihua and Knudsen, Karen-Luise and Knudsen, Mads F.},
