@article{askeyevFishAssemblagesLakes2023,
  title = {Fish Assemblages in Lakes along Environmental Gradients at the Eastern Edge of {{Europe}}},
  author = {Askeyev, Oleg and Monakhov, Sergey and Askeyev, Igor and Askeyev, Arthur and Sparks, Tim H.},
  year = {2023},
  month = jun,
  journal = {Environmental Biology of Fishes},
  volume = {106},
  number = {6},
  pages = {1265--1276},
  issn = {1573-5133},
  doi = {10.1007/s10641-023-01414-0},
  urldate = {2025-06-10},
  abstract = {Fish assemblages and environmental variables were studied in 155 lake and pond sites on the east edge of Europe (Tatarstan Republic). The main aim of the work was to describe and identify patterns of fish distribution in lakes from a poorly studied region of Europe. The impact of environmental factors on fish species and communities was analyzed with regression and ordination methods. Fish assemblages, probability of occurrences, and abundance of many fish species were very different between our results at the eastern edge of Europe and those reported from further west in the continent. The size (area) of lakes was the main environmental variable (positively) influencing total abundance, the total number of fish species, and the Shannon diversity index. The probability of occurrence of more than half of the fish species was related to the area and bottom substrate, either on its own or in combination with elevation and water depth. For the first time at this eastern edge of Europe, means and standard deviations for fish species along basic environmental gradients were estimated. The latter were highly variable between species.},
  langid = {english},
  keywords = {Applied Ecology,Ecology,Environmental Geography,Environmental gradients,Fish and Wildlife Biology,Freshwater and Marine Ecology,Lakes,Limnology,Probability of occurrence,Republic of Tatarstan},
