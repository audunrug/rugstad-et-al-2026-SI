@article{fanfarilloEffectsConventionalOrganic2022,
  title = {Effects of Conventional and Organic Management on Plant and Insect Communities in a Traditional Elephant Garlic Crop},
  author = {Fanfarillo, Emanuele and Calabrese, Daniele and Angiolini, Claudia and Bacaro, Giovanni and Biagiotti, Stefano and Castagnini, Paolo and Loppi, Stefano and Martellini, Tommaso and Maccherini, Simona},
